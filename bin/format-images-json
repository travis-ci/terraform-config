#!/usr/bin/env ruby
# frozen_string_literal: true

require 'json'

def main(argv: ARGV)
  generated = {}

  argv.each do |kv|
    image, languages_string = kv.split('=')
    languages_string.split(',').each do |language|
      generated[language] = image
    end
  end

  $stdout.puts JSON.pretty_generate(generated)
end

main if $PROGRAM_NAME == __FILE__
